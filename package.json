    "eslint": "^9.10.0",
    "jiti": "^1.21.6",
    "json-schema-to-typescript": "^15.0.2",
    "pkgroll": "^2.4.2",
    "tsx": "^4.19.0",
    "vitest": "^2.0.5"
  }
